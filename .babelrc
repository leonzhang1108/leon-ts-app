{
	"presets": [
		"@babel/preset-react",
		["@babel/preset-env", {
			"modules": false
		}]
	],
	"plugins": [
		"react-hot-loader/babel",
		"@babel/plugin-proposal-class-properties",
		["import", { // 导入一个插件
			"libraryName": "antd", // 暴露的库名
			"style": true // 直接将ants样式文件动态编译成行内样式插入，就不需要每次都导入
		}]
	]
}